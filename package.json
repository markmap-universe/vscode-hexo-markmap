{
  "publisher": "maxchang3",
  "name": "vscode-hexo-markmap",
  "displayName": "vscode-hexo-markmap",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.cjs",
  "engines": {
    "vscode": "^1.89.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "markdown.markdownItPlugins": true,
    "markdown.previewScripts": [
      "./dist/markmap-init.global.js"
    ],
    "markdown.previewStyles": [
      "./dist/style.css"
    ]
  },
  "scripts": {
    "build": "tsup --env.NODE_ENV production --treeshake",
    "dev": "tsup --watch ./src --env.NODE_ENV development",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "pnpm run build"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "markmap-toolbar": "^0.18.10",
    "radashi": "^12.4.0",
    "reactive-vscode": "^0.2.13",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@node-rs/xxhash": "^1.7.6",
    "gray-matter": "^4.0.3",
    "markmap-common": "^0.18.9",
    "markmap-lib": "^0.18.11",
    "markmap-view": "^0.18.10",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
