{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "skipLibCheck": true,
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [ "src/*" ]
    },
  },
  "exclude": [
    "src/preview/**",
    "node_modules",
  ]
}
